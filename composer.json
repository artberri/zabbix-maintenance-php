    "type": "library",
    "license": "MIT",
    "keywords": ["zabbix", "cli", "maintenance"],
    "homepage": "https://github.com/artberri/zabbix-maintenance-php",
    "authors": [
        {
            "name": "Alberto Varela",
            "email": "alberto@berriart.com",
            "homepage": "http://www.berriart.com"
        }
